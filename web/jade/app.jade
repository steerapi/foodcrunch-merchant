//Manage Discounts
#pageManageDiscounts(data-role='page', ng-controller="Graph")
  div(data-theme='a', data-role='header')
    h3
      | Discount Adjustment
  div(data-role='content')
    h3 {{dow}}
    #placeholder(style="width:300px;height:300px")
    .row-fluid
      .span2
        a(data-role='button', ng-click='increase()')
          | Increase
      .span2
        a(data-role='button', ng-click='decrease()')
          | Decrease
      .span2
        a(data-role='button', ng-click='prev()')
          | Prev
      .span2
        a(data-role='button', ng-click='next()')
          | Next
    div(data-role='fieldcontain')
      label(for='selectmenu7')
        | Day
      select(ng-model="dow",name='', data-theme='b')
        option(value='Monday')
          | Monday
        option(value='Tuesday')
          | Tuesday
        option(value='Wednesday')
          | Wednesday
        option(value='Thursday')
          | Thursday
        option(value='Friday')
          | Friday
        option(value='Saturday')
          | Saturday
        option(value='Sunday')
          | Sunday
